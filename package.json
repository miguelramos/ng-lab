{
  "name": "ng-lab-sdk",
  "version": "0.4.1",
  "author": {
    "name": "Miguel Ramos",
    "email": "miguel.marques.ramos@gmail.com",
    "url": "https://ng-lab.github.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/miguelramos/ng-lab"
  },
  "homepage": "https://github.com/miguelramos/ng-lab",
  "bugs": {
    "url": "https://github.com/miguelramos/ng-lab/issues"
  },
  "keywords": [
    "angular",
    "library",
    "ng-lab",
    "support"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:support && npm run build:configurator && npm run build:responsive && npm run build:hub && npm run build:jwt && npm run build:http-url && npm run build:http-script && npm run build:layout",
    "build:support": "ng build support --prod",
    "build:configurator": "ng build configurator --prod",
    "build:responsive": "ng build responsive --prod",
    "build:hub": "ng build hub --prod",
    "build:jwt": "ng build jwt --prod",
    "build:http-url": "ng build http-url --prod",
    "build:http-script": "ng build http-script --prod",
    "build:layout": "ng build layout --prod",
    "test": "ng test --codeCoverage",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "commit": "git-cz",
    "deploy": "ng build ng-lab-demo --aot --baseHref=/ng-lab/ --buildOptimizer",
    "release": "release-it --config=./.release-it.json --increment=minor --verbose",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@ngxs-labs/data": "^2.4.1",
    "@ngxs/store": "^3.6.1",
    "@nrwl/angular": "9.0.4",
    "@types/lodash": "^4.14.123",
    "@types/prismjs": "^1.16.0",
    "core-js": "^2.5.4",
    "lodash": "^4.17.11",
    "ngx-markdown": "^9.0.0",
    "prismjs": "^1.17.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular-devkit/build-ng-packagr": "~0.900.5",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/language-service": "^9.0.5",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@ngrx/schematics": "7.2.0",
    "@ngxs/devtools-plugin": "^3.6.1",
    "@nrwl/jest": "9.0.4",
    "@nrwl/workspace": "9.0.4",
    "@release-it/conventional-changelog": "^1.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "24.0.9",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.16.0",
    "codelyzer": "^5.1.2",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "6.2.0",
    "font-awesome": "^4.7.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.1.0",
    "jest-preset-angular": "8.0.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.0.0",
    "prettier": "1.19.1",
    "prismjs": "^1.16.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  }
}
