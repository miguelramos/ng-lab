		"@types/jasminewd2": "~2.0.3",
		"@types/jest": "^23.0.0",
		"@types/node": "~8.9.4",
		"automatic-release": "^1.1.1",
		"codelyzer": "~4.5.0",
		"cz-conventional-changelog": "^2.1.0",
		"husky": "^1.3.1",
